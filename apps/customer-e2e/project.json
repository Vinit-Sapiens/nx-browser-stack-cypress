{
  "name": "customer-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/customer-e2e/src",
  "projectType": "application",
  "targets": {
    "browserstack": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "cd apps/customer-e2e/",
          "yarn browserstack-cypress run --sync --cf ./apps/customer-e2e/browserstack.json"
        ],
        "parallel": false
      }
    },
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/customer-e2e/cypress.config.ts",
        "testingType": "e2e"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/customer-e2e/**/*.{js,ts}"
        ]
      }
    }
  },
  "tags": [],
  "implicitDependencies": [
    "customer"
  ]
}